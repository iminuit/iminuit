      "sigma[amp]  : HESSE =  100.0, JAX =   70.7\n",
      "sigma[mu]   : HESSE = 0.0100, JAX = 0.0071\n",
      "sigma[sigma]: HESSE = 0.0071, JAX = 0.0050\n"
     ]
