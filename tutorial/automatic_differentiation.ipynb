   "source": [
    "# define the score function\n",
    "@jit\n",
    "def score(par):\n",
    "    result = 0.0\n",
    "    for xi, yi in zip(data_x, data_y):\n",
    "        y_var = sigma_y ** 2 + (f_prime(xi, par) * sigma_x) ** 2\n",
    "        result += (yi - f(xi, par)) ** 2 / y_var\n",
    "    return result\n",
    "\n",
    "# compile and test the jit-ed function\n",
    "score(np.zeros(3))"
   ]
